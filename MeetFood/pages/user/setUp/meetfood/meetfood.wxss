.meetfood {
  width: 100%;
  height: 100vh;
}
.meetfood .mian {
  padding: 40rpx;
}
.meetfood .mian .logo {
  width: 120rpx;
  height: 120rpx;
  margin: 80rpx auto 0;
  border-radius: 20rpx;
}
.meetfood .mian .logo image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.meetfood .mian .content {
  margin: 40rpx auto;
  text-align: center;
}
.meetfood .mian .content .name {
  font-size: 32rpx;
  font-weight: 600;
  color: #333;
  margin: 0 0 10rpx;
}
.meetfood .mian .content .version {
  font-size: 26rpx;
  color: #333;
}
.meetfood .mian .copyright-notice {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 100rpx;
  text-align: center;
  font-size: 22rpx;
  color: #999;
}
